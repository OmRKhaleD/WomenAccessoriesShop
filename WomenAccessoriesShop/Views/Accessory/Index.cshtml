@model IEnumerable<Accessory>

@foreach (var item in Model)
{
<div class="col-sm-3 col-lg-3 col-md-3">
    <div class="thumbnail">
        <img src="@item.ImageUrl" alt="" style="width:200px; height:250px;">
        <div class="caption">
            <h3 class="pull-right">@item.Price.ToString("c")</h3>
            <h3>
                <a>@item.Name</a>
            </h3>
            <p>@item.Description</p>
        </div>
    </div>
</div>
}

