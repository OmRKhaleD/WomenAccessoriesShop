@model Accessory

<div class="col-sm-3 col-lg-3 col-md-3">
    <div class="thumbnail">
        <img src="@Model.ImageUrl" alt="" style="width:200px; height:250px;">
        <div class="caption">
            <h3 class="pull-right">@Model.Price.ToString("c")</h3>
            <h3>
                <a>@Model.Name</a>
            </h3>
            <p>@Model.Description</p>
        </div>
    </div>
</div>